#include <iostream>
#include <cstring>
using namespace std;
class Rabotnik
{
private:
    char ime[30];
    char prezime[30];
    int plata;
public:
    Rabotnik() {}
    Rabotnik(char i[], char p[], int pl)
    {
        strcpy(ime,i);
        strcpy(prezime,p);
        plata = pl;
    }
    int getPlata()
    {
        return plata;
    }
    void print()
    {
        cout << ime << " " << prezime << " " << plata << endl;
    }
};
class Fabrika
{
private:
    Rabotnik rabotnik[100];
    int brojVraboteni;
public:
    Fabrika() {}
    Fabrika(Rabotnik r[], int broj)
    {
        brojVraboteni = broj;
        for(int i = 0; i < brojVraboteni; i++)
        {
            rabotnik[i] = r[i];
        }
    }
    void printVraboteni()
    {
        cout << "Site vraboteni: " << endl;
        for(int i = 0; i < brojVraboteni; i++)
        {
            rabotnik[i].print();
        }
    }
    void printSoPlata (int plata)
    {
        cout << "Vraboteni so plata povisoka od " << plata << " :" << endl;
        for (int i = 0; i < brojVraboteni; i++)
        {
            if(rabotnik[i].getPlata() > plata)
            {
                rabotnik[i].print();
            }
        }
    }

};
int main()
{
    int n;
    cin >> n;
    char ime[30], prezime[30];
    int plata;
    Rabotnik rabotnik[100];
    for(int i = 0; i < n; i++)
    {
        cin >> ime >> prezime >> plata;
        rabotnik[i] = Rabotnik(ime, prezime, plata);
    }
    int minPlata;
    cin >> minPlata;
    Fabrika fabrika(rabotnik, n);
    fabrika.printVraboteni();
    fabrika.printSoPlata(minPlata);
    return 0;

}
